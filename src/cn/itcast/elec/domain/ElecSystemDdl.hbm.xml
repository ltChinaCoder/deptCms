<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >

    <class name="cn.itcast.elec.domain.ElecSystemDdl" table="Elec_SystemDdl">
        <id name="seqID"  type="int">
        <column name="seqID" not-null="true" ></column>
            <generator class="increment">
    
            </generator>
            
        </id>         
        <property name="keyword" not-null="true">
        <column name="keyword" sql-type="varchar(50)"> </column>
        </property>
        <property name="ddlCode" type="int">
        <column name="ddlCode" ></column>
        </property>
        <property name="ddlName" >
        <column name="ddlName" sql-type="varchar(50)"></column>
        </property>
    </class>

</hibernate-mapping>
